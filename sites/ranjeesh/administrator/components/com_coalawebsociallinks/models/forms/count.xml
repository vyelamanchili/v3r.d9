<?xml version="1.0" encoding="utf-8"?>
<form>
    <fieldset addfieldpath="/administrator/components/com_categories/models/fields">
        <field 
            name="id"
            type="text"
            label="COM_CWSOCIALLINKS_FIELD_ID_LABEL"
            description="COM_CWSOCIALLINKS_FIELD_ID_DESC"
            maxlength="10"
            filter="INTEGER"
            class="readonly"
            readonly="true"/>
        
        <field
            name="title"
            type="text"
            class="inputbox"
            size="40"
            label="JGLOBAL_TITLE"
            description="JFIELD_TITLE_DESC"/>       
        
        <field 
            name="alias" 
            type="text" 
            class="inputbox"
            size="40" 
            label="JFIELD_ALIAS_LABEL"
            description="COM_CWSOCIALLINKS_FIELD_ALIAS_DESC" />
        
        <field
            name="url"
            type="text"
            maxlength="200"
            validate="url"
            label="COM_CWSOCIALLINKS_FIELD_URL_LABEL"
            description="COM_CWSOCIALLINKS_FIELD_URL_DESC" />
        
        <field
            name="tm"
            label="COM_CWSOCIALLINKS_FIELD_TM_LABEL"
            description="COM_CWSOCIALLINKS_FIELD_TM_DESC"
            type="text"/>
        
        <field
            name="facebook_comment"
            label="COM_CWSOCIALLINKS_FIELD_FC_LABEL"
            description="COM_CWSOCIALLINKS_FIELD_FC_DESC"
            type="text"
            maxlength="20"/>
        
        <field
            name="facebook_like"
            label="COM_CWSOCIALLINKS_FIELD_FL_LABEL"
            description="COM_CWSOCIALLINKS_FIELD_FL_DESC"
            type="text"
            maxlength="200"/>
        
        <field
            name="facebook_share"
            label="COM_CWSOCIALLINKS_FIELD_FS_LABEL"
            description="COM_CWSOCIALLINKS_FIELD_FS_DESC"
            type="text"
            maxlength="200"/>
        
        <field
            name="facebook_total"
            label="COM_CWSOCIALLINKS_FIELD_FT_LABEL"
            description="COM_CWSOCIALLINKS_FIELD_FT_DESC"
            type="text"
            maxlength="200"/>
        
        <field
            name="google"
            label="COM_CWSOCIALLINKS_FIELD_G_LABEL"
            description="COM_CWSOCIALLINKS_FIELD_G_DESC"
            type="text"
            maxlength="50"/>
        
        <field
            name="linkedin"
            label="COM_CWSOCIALLINKS_FIELD_L_LABEL"
            description="COM_CWSOCIALLINKS_FIELD_L_DESC"
            type="text"
            maxlength="50"/>
        
        <field
            name="pinterest"
            label="COM_CWSOCIALLINKS_FIELD_P_LABEL"
            description="COM_CWSOCIALLINKS_FIELD_P_DESC"
            type="text"
            maxlength="50"/>
        
        <field
            name="reddit"
            label="COM_CWSOCIALLINKS_FIELD_R_LABEL"
            description="COM_CWSOCIALLINKS_FIELD_R_DESC"
            type="text"
            maxlength="50"/>
        
        <field
            name="stumbleupon"
            label="COM_CWSOCIALLINKS_FIELD_S_LABEL"
            description="COM_CWSOCIALLINKS_FIELD_S_DESC"
            type="text"
            maxlength="50"/>
        
        <field
            name="state"
            type="list"
            label="JSTATUS"
            description="COM_CWSOCIALLINKS_FIELD_STATE_DESC"
            class="inputbox"
            size="1"
            default="1">
            <option value="1">JPUBLISHED</option>
            <option value="0">JUNPUBLISHED</option>
            <option value="2">JARCHIVED</option>
            <option value="-2">JTRASHED</option>
        </field>
        
        <field
            name="modified"
            type="calendar"
            format="%Y-%m-%d %H:%M:%S"
            filter="server_utc"
            label="JGLOBAL_FIELD_MODIFIED_LABEL"
            description="JGLOBAL_FIELD_MODIFIED_LABEL"/>
        <field
            name="modified_by"
            type="user"
            label="JGLOBAL_FIELD_MODIFIED_BY_LABEL"
            description="JGLOBAL_FIELD_MODIFIED_BY_DESC"/>
    </fieldset>
</form>