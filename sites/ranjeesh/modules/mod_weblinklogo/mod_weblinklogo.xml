						<option value="o_asc">MOD_WEBLINKLOGO_VALUE_ORDERASC</option>
						<option value="o_dsc">MOD_WEBLINKLOGO_VALUE_ORDERDESC</option>
						<option value="n_asc">MOD_WEBLINKLOGO_VALUE_BYNAMEASC</option>
						<option value="n_dsc">MOD_WEBLINKLOGO_VALUE_BYNAMEDESC</option>
					</field>
				</fieldset>
				
				<fieldset name="selection_tags" label="MOD_WEBLINKLOGO_SELECTIONTAGS_FIELDSET_LABEL">
				
